<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tumo.model.dao.UserDao">

	<insert id="createUser"  parameterType="userdto">
		insert into user (nickname, email, password, introduce)
			values (#{nickname},#{email},#{password},#{introduce})
	</insert>
	
	<select id="checkEmail" parameterType="string" resultType="userdto">
		select * from user
			where email = #{email} 
	</select>
	
	<select id="checkNickname" parameterType="string" resultType="userdto">
		select * from user
			where nickname = #{nickname} 
	</select>
	
</mapper>